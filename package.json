{
  "name": "wheel-fortune",
  "version": "2.0.0",
  "description": "WheelFortune implements an animation of a spinning wheel of fortune using the GSAP library. It allows customization of parameters including the number of segments, number of revolutions, spin states, and callbacks.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/wheel-fortune.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/wheel-fortune/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/wheel-fortune",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "build": "yarn clean && parcel build",
    "lint": "eslint **/*.{ts,tsx,js,jsx}",
    "prettier": "prettier --write **/*.{ts,tsx,js,jsx}",
    "lintfix": "yarn lint --fix && yarn prettier"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "eslint": "^9.12.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "gsap": "^3.12.5"
  },
  "keywords": [
    "wheel of fortune",
    "spin wheel",
    "spinner",
    "spinning wheel",
    "wheel animation",
    "gsap",
    "typescript",
    "javascript",
    "game",
    "roulette",
    "fortune wheel",
    "customizable",
    "interactive",
    "animation",
    "effects"
  ]
}
