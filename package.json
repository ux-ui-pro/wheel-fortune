    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^7.32.0 || ^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "parcel": "^2.12.0",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "wheel",
    "fortune",
    "spin",
    "animation",
    "gsap",
    "customEase",
    "game",
    "wheel-of-fortune",
    "rotating-wheel",
    "libration",
    "web-animation",
    "javascript",
    "typescript",
    "es6",
    "html",
    "css"
  ]
}
